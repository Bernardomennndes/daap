{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "start:dev": "turbo run start:dev --filter=@daap/cache-service --filter=@daap/reviews-service --filter=@daap/search-service",
    "start:services": "turbo run start --filter=@daap/cache-service --filter=@daap/reviews-service --filter=@daap/search-service",
    "build:services": "turbo run build --filter=@daap/cache-service --filter=@daap/reviews-service --filter=@daap/search-service",
    "db:create-indexes": "bash scripts/create-text-indexes.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:integration": "node scripts/test-integration.js",
    "k6:install": "docker pull grafana/k6:0.49.0",
    "k6:test": "bash scripts/run-k6-test.sh",
    "k6:test:stress": "bash scripts/run-k6-test.sh --script cache-stress-test.js",
    "k6:test:lfu": "bash scripts/run-k6-test.sh --strategy lfu",
    "k6:test:lru": "bash scripts/run-k6-test.sh --strategy lru",
    "k6:test:hybrid": "bash scripts/run-k6-test.sh --strategy hybrid",
    "k6:compare": "bash scripts/run-k6-strategy-comparison.sh",
    "k6:clean": "rm -rf packages/tools/k6/results/*.json packages/tools/k6/results/*.html packages/tools/k6/results/*.txt"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.4.4",
    "typescript": "~5.4.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.4.1"
}
